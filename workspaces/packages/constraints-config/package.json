{
  "name": "@joostvdwsd/constraints-config",
  "packageManager": "yarn@4.0.2",
  "devDependencies": {
    "@types/node": "^22.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@yarnpkg/types": "^4.0.0"
  },
  "main": "./src/index.ts",
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "prepack": "rm -f tsconfig.tsbuildinfo && yarn tsc --noEmit false",
    "postpack": "rm -rf ./lib"
  },
  "exports": {
    ".": "./src/index.ts",
    "./local": "./local-include.cjs"
  },
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joostvdwsd/common-tools",
    "directory": "workspaces/packages/constraints-config"
  },
  "version": "0.0.0"
}
